{
  "name": "shopping-list-preact-pouchdb",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build --no-prerender --template=./public/index.html --dest=./docs --clean",
    "test": "eslint src && preact test",
    "serve": "preact build --no-prerender && preact serve",
    "dev": "preact watch --template=./public/index.html --config=./preact.config.js"
  },
  "babel": {
    "plugins": [
      [
        "transform-react-jsx",
        {
          "pragma": "h"
        }
      ]
    ]
  },
  "keywords": [],
  "author": "rrsingh@us.ibm.com",
  "license": "Apache 2.0",
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "devDependencies": {
    "eslint": "^4.6.1",
    "eslint-config-synacor": "^1.1.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "if-env": "^1.0.0",
    "postcss": "^6.0.11",
    "preact-cli": "^1.4.1"
  },
  "dependencies": {
    "ibm-shopping-list-model": "^0.2.1",
    "immutable": "^3.8.1",
    "materialize-css": "^0.100.2",
    "pouchdb": "^6.3.4",
    "pouchdb-find": "^6.3.4",
    "preact": "^8.2.5",
    "prop-types": "^15.5.10"
  }
}
